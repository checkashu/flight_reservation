<h1 align="center">Listing All Users</h1>
<div align="center">
<% @users.each do |user| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li class="flight-number">
          <%= link_to "#{user.name}", user_path(user) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>
</div>